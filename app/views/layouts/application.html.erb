<!DOCTYPE html>
<html>
<head>
  <title>Chat</title>
  <%= javascript_pack_tag 'application' %>
  <%#= stylesheet_link_tag 'application' %>
  <%= csrf_meta_tags %>
</head>

<body>
  <div class="container">
    <% flash.each do |key, value| %>
      <div class="alert alert-<%= key %>"><%= value %></div>
    <% end %>
  </div>
  <%= yield %>
</body>
</html>